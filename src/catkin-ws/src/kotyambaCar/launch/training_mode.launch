    user="pi" password="321" 
    env-loader="$(env KOTYAMBA_REPO_RASPBERRY)/src/catkin-ws/src/kotyambaCar/scripts/setup_raspberry_pi_nodes.sh" >
  </machine>

  <!-- <node pkg="kotyambaCar" type="command_listener.py" name="command_listener" machine="raspberry_pi"/>
